package com.io.app;

class A {
	static void staticMethod() {
		System.out.println("Static Method");
	}
	

    int methodOfA()
    {
        return (true ? null : 0);
        //return null;
//        return (false ? null : 0);


    }
}

public class MainClass {
	public static void main(String[] args) {
		A a = null;

		a.staticMethod();
		
		a=new A();
		a.methodOfA();
		
	}
	
	
}
