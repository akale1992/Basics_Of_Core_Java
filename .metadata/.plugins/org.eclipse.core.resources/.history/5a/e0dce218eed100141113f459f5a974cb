package com.jlc.test;
	import java.util.Scanner;

	public class Myfibonaci {

	   public static void main(String[] args) {
	      //	  Scanner scanner = new Scanner(System.in);
	      // System.out.print("Enter the range of fibbonacci");
	      // int n = scanner.nextInt();
		  int n =15;
	       int [] fibo = new int[n];
		   fibo[0]=0;
		   fibo[1] =1;
		   for(int i=2;i<n;i++){
		     fibo[i] =fibo[i-1] +fibo [i-2];
		   }
		    System.out.print("Fibonaci series upto range"+n+"is ");
		   for(int i=0;i<n;i++){
		       System.out.print(fibo[i]+" ");
		   }
	}
	}


